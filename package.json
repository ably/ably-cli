{
  "name": "ably-cli",
  "version": "0.0.1",
  "description": "Ably CLI for Pub/Sub, Chat, Spaces and the Control API",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && rm -f tsconfig.tsbuildinfo && tsc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "author": "",
  "license": "ISC",
  "bin": {
    "ably": "./bin/run.js"
  },
  "type": "module",
  "oclif": {
    "bin": "ably",
    "dirname": "ably",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": ["./dist"]
  },
  "dependencies": {
    "@oclif/core": "^4.2.10",
    "ably": "github:ably/ably-js#type-correction-publishoptions"
  },
  "devDependencies": {
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.13.11",
    "chai": "^4",
    "mocha": "^11",
    "shx": "^0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ]
}